function MChest_100()
    local mysticIsland = game:GetService("Workspace").Map:FindFirstChild("MysticIsland")

    if mysticIsland then
        for i, v in pairs(mysticIsland:GetChildren()) do
            if v.Name:match("FragChest") and (v.Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).Magnitude <= 100 then
                _G.MChest_100 = v
                return
            end
        end
    end
end
